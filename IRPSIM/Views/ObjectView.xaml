<UserControl x:Class="IRPSIM.Views.ObjectView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:viewmodel="clr-namespace:IRPSIM.ViewModels"
              mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <TreeView>
        <TreeViewItem Header="Supply" ItemsSource="{Binding Variables}" />
        <TreeViewItem Header="Demand"/>
        <TreeViewItem Header="Storage"/>
        <TreeViewItem Header="Scenarios"/>
        <TreeViewItem Header="Scripts"/>
        <TreeViewItem Header="Categories"/>
        <TreeViewItem Header="Definitions"/>
        <TreeViewItem Header="Variables"/>
    </TreeView>
</UserControl>
